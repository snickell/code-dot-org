      }
    },
    {
      "key": "e792cb4e-dc6b-4b13-b0fd-22cda6c36ddc",
      "position": 2,
      "properties": {
        "progression_name": "Wrap Up"
      },
      "seeding_key": {
        "activity_section.key": "e792cb4e-dc6b-4b13-b0fd-22cda6c36ddc",
        "lesson_activity.key": "61acb818-3a35-42e9-b47d-a137bdf6a02e"
