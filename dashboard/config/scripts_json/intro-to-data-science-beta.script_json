      "key": "95defd82-e848-4596-9444-7fdf2c896277",
      "position": 1,
      "properties": {
        "description": "<i class=\"fa fa-check-square-o\" aria-hidden=\"true\"></i> **Do This:** Introduce data visualizations and explain that these are used to quickly identify trends and patterns."
      },
      "seeding_key": {
        "activity_section.key": "95defd82-e848-4596-9444-7fdf2c896277",
        "lesson_activity.key": "94c9c4b7-18f9-4434-835c-36d8b752b02c"
      }
